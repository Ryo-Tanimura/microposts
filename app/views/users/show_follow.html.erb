<div class="panel panel-default">
  <div class="panel-heading">
    <h3><%= @title %></h3>
  </div>
    
  <div class="panel-body">
    <% @users.each do |user| %>
      <li>
        <%= gravatar_for user, size: 50 %>
        <%= link_to user.name, user %>
        <br>
      </li>
      <div class="clearfix margin-b-10"></div>
    <% end %>
  </div>
</div>